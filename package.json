    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "start:dev": "cd src && npx ts-node -r tsconfig-paths/register src/main.ts",
        "start:api": "cd src && npx nest start --watch",
        "prisma:generate": "cd src && npx prisma generate",
        "prisma:migrate": "cd src && npx prisma migrate dev",
        "test": "jest --runInBand --config src/jest.config.js",
        "test:unit": "jest src/test/unit --runInBand --config src/jest.config.js",
        "test:frontend": "jest --config jest.config.frontend.js --runInBand",
        "test:integration": "jest src/test/integration --runInBand --config src/jest.config.js",
        "test:e2e": "jest src/test --testPathPattern=\\.e2e-spec\\.ts$ --runInBand --detectOpenHandles --config src/jest.config.js",
        "test:multi-tenant": "jest src/test/integration/multi-tenant.e2e-spec.ts --runInBand --detectOpenHandles --config src/jest.config.js",
        "test:coverage": "jest --coverage --runInBand --config src/jest.config.js",
        "test:ci": "jest --ci --coverage --maxWorkers=2 --config src/jest.config.js"
    }
